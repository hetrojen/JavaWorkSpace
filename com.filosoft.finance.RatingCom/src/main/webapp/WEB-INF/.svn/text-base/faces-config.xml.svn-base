         <navigation-case> 
      <from-outcome>raporUret</from-outcome> 
      <to-view-id>/RaporGeneratorView.jsf</to-view-id> 
    </navigation-case>
	</navigation-rule>

	<managed-bean>
	 <managed-bean-name>ratingView</managed-bean-name>
	 <managed-bean-class>com.company.rating.view.RatingView</managed-bean-class>
	 <managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
 <lifecycle>
  <phase-listener>com.company.rating.util.AccessControlPhaseListener</phase-listener>
 </lifecycle>

</faces-config>
