    </navigation-case> 
      <navigation-case> 
      <from-outcome>kantitatifDegerlendirme</from-outcome> 
      <to-view-id>/degerlendirme/KantitatifDegerlendirmeView.jsf</to-view-id> 
    </navigation-case> 
         <navigation-case> 
      <from-outcome>kalitatifDegerlendirme</from-outcome> 
      <to-view-id>/degerlendirme/KalitatifDegerlendirmeView.jsf</to-view-id> 
    </navigation-case> 
      <navigation-case> 
      <from-outcome>sonucDegerlendirme</from-outcome> 
      <to-view-id>/degerlendirme/DegerlendirmeSonucuView.jsf</to-view-id> 
