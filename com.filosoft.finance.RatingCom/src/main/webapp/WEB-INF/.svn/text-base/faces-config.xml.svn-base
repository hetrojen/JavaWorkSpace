      <to-view-id>/LoginView.jsf</to-view-id> 
    </navigation-case> 
    <navigation-case> 
      <from-outcome>startRasyo</from-outcome> 
      <to-view-id>/RatingMainPage.jsf</to-view-id> 
    </navigation-case> 
	  <navigation-case> 
      <from-outcome>yatayAnaliz</from-outcome> 
      <to-view-id>/grafik/YatayAnalizGrafikView.jsf</to-view-id> 
    </navigation-case> 
	  	  <navigation-case> 
      <from-outcome>dikeyAnaliz</from-outcome> 
      <to-view-id>/grafik/DikeyAnalizGrafikView.jsf</to-view-id> 
    </navigation-case> 
	      <navigation-case> 
      <from-outcome>gelirBil</from-outcome> 
      <to-view-id>/GelirBilancoMan.jsf</to-view-id> 
    </navigation-case> 
      <navigation-case> 
      <from-outcome>rasyoAnalizGrafik</from-outcome> 
      <to-view-id>/grafik/RasyoAnalizGrafikView.jsf</to-view-id> 
    </navigation-case> 
    
       <navigation-case> 
      <from-outcome>rasyoAnalizRasyo</from-outcome> 
      <to-view-id>/grafik/RasyoAnalizRasyoView.jsf</to-view-id> 
    </navigation-case> 
          <navigation-case> 
      <from-outcome>firmaTanimlama</from-outcome> 
