<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:p="http://primefaces.prime.com.tr/ui"
	template="../templates/mainTemplate.xhtml">

	<ui:define name="center">
		<p:panel title="#{generalData.donem} dönemi #{generalData.yil} yılına ait DEĞERLENDİRME SONUCU" id="sonucPanel" >
					<h:panelGrid columns="2" id="sonucPa">
						<h:outputLabel value="KANTİTATİF DEĞERLENDİRME SONUÇ:"></h:outputLabel>
						<h:inputText style="text-align:right"  value="#{sesionBean.ratingViewForm.kantitatifToplam}" disabled="true">
						  <f:convertNumber pattern="#,#0.0"
														locale="#{sesionBean.trlocal}" />
						</h:inputText>
						<h:outputLabel value="KALİTATİF DEĞERLENDİRME SONUÇ:"></h:outputLabel>

						<h:inputText  style="text-align:right"  value="#{sesionBean.ratingViewForm.kalitatifToplam}" disabled="true">
						<f:convertNumber pattern="#,#0.0"
														locale="#{sesionBean.trlocal}" />
						</h:inputText>
						<h:outputLabel value="TOPLAM SONUÇ:"></h:outputLabel>
						<h:inputText style="text-align:right"  value="#{degerlendimeSonuc.toplamPuan}" disabled="true">
					      <f:convertNumber pattern="#,#0.0"	locale="#{sesionBean.trlocal}" />
						</h:inputText>
						<!-- h:panelGrid columns="4">
							<h:outputLabel value="dönem..:"></h:outputLabel>
							<h:outputText  value="#{generalData.donem}"></h:outputText>
					
							<h:outputLabel value="yıl..:"></h:outputLabel>
								<h:outputText value="#{generalData.yil}"></h:outputText>
						</h:panelGrid-->
                        <h:panelGrid columns="1">
                        <!--p:commandButton value="Hesapla" update="sonucPa"></p:commandButton-->
                        <!-- p:commandButton update="degerlendirme" value="Kaydet"></p:commandButton-->
                        </h:panelGrid>
						

					</h:panelGrid>
                    <p:panel  header="#{degerlendimeSonuc.derecelendirmePanelHeader}">
                       <h:panelGrid columns="2">
                             <h:panelGrid columns="1">
                               <h:panelGrid columns="2" >
                               
                                 <h:panelGrid width="300">
                                 <h:outputText  value="#{degerlendimeSonuc.toplamDerece}" styleClass="#{degerlendimeSonuc.derecelendirmeStyle}"></h:outputText>
                                    </h:panelGrid>
                                      
                                      <h:panelGrid columns="1" >
                       
                              <h:outputLabel  value="#{degerlendimeSonuc.descriptionNot}" styleClass="header"></h:outputLabel>
                                <h:outputLabel  value="#{degerlendimeSonuc.descriptionFirma}"></h:outputLabel>
                             
                             </h:panelGrid> 
                               
                               </h:panelGrid> 
                             
                               
                               
                               <h:panelGrid columns="2" width="300">
                                 <h:outputLabel value="Kantitatif Derece:#{degerlendimeSonuc.kantitatifDerece}"></h:outputLabel>
 
                               </h:panelGrid>
                                 <h:panelGrid columns="2" width="350">
                                 <h:outputLabel value="Kalitatif Derece:#{degerlendimeSonuc.kalitatifDerece}"></h:outputLabel>
                                
                               </h:panelGrid>
                             </h:panelGrid>
                      
                       
                       </h:panelGrid>
                     
                    
                    </p:panel>

				</p:panel>
	</ui:define>

</ui:composition>