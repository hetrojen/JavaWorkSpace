					<p:column>

						<p:graphicImage value="#{varlik.arrowImgPath}"></p:graphicImage>
					</p:column>

					<p:column>
					  <h:panelGrid style="width:220px; text-align:left;">
				  <h:outputLabel value="#{varlik.name}" styleClass="#{varlik.nameStyle}" />
					  </h:panelGrid>
						
					</p:column>

					<p:column>
						<f:facet name="header">
							<h:outputText value="Önceki Dönem"></h:outputText>
						</f:facet>
						<h:inputText value="#{varlik.onceki}" disabled="true"
							style="text-align:right" size="15">

							<f:convertNumber pattern="#,##0.00"
								locale="#{sesionBean.trlocal}" />

						</h:inputText>
					</p:column>

					<p:column>
						<f:facet name="header">
							<h:outputText value="Cari Dönem"></h:outputText>
						</f:facet>
						<h:inputText value="#{varlik.cari}" disabled="true"
