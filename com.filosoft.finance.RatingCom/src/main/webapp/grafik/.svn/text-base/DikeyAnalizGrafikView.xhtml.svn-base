<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:p="http://primefaces.prime.com.tr/ui"
	template="../templates/mainTemplate.xhtml">

	<ui:define name="center">
	<h:form>
        <p:tabView dynamic="true">
           <p:tab title="Aktif Dağılım">
              
             <h:panelGrid columns="2">    
                    <p:dataTable  value="#{grafikData.donenVarliklarGrafikD.grafikModels}" var="varlik" style="width:550px">
                           	<p:column style="width:390px">
                           		<f:facet name="header">
									<h:outputText styleClass="outputL" value="Dönen Varlıklar"></h:outputText>
								</f:facet>
								<h:outputText styleClass="outputL"  value="#{varlik.name}" />
							</p:column>

							<p:column>
								<f:facet name="header">
									<h:outputText styleClass="outputL" value="Cari Dönem"></h:outputText>
								</f:facet>
								<h:inputText value="#{varlik.cari}" disabled="true"	style="text-align:right" size="15">

									<f:convertNumber pattern="#,##0.00"
										locale="#{sesionBean.trlocal}" />
									
								</h:inputText>
							</p:column>
								<p:column>
								<f:facet name="header">
									<h:outputText styleClass="outputL" value="Yüzde(%)"></h:outputText>
								</f:facet>
								<h:inputText value="#{varlik.percent}" disabled="true"	style="text-align:right" size="5">

																		
								</h:inputText>
							</p:column>
                    
                    </p:dataTable>
             <h:graphicImage   value="#{grafikData.donenVarliklarGrafikD.grafik}" />
          
                    <p:dataTable  value="#{grafikData.duranVarliklarGrafikD.grafikModels}" var="varlik" style="width:550px">
                           	<p:column style="width:390px">
                           		<f:facet name="header">
									<h:outputText styleClass="outputL" value="Duran Varlıklar"></h:outputText>
								</f:facet>
								<h:outputText styleClass="outputL" value="#{varlik.name}" />
							</p:column>

							<p:column>
								<f:facet name="header">
									<h:outputText styleClass="outputL" value="Cari Dönem"></h:outputText>
								</f:facet>
								<h:inputText value="#{varlik.cari}" disabled="true"	style="text-align:right" size="15">

									<f:convertNumber pattern="#,##0.00"
										locale="#{sesionBean.trlocal}" />
									
								</h:inputText>
							</p:column>
								<p:column>
								<f:facet name="header">
									<h:outputText styleClass="outputL" value="Yüzde(%)"></h:outputText>
								</f:facet>
								<h:inputText value="#{varlik.percent}" disabled="true"	style="text-align:right" size="5">

																		
								</h:inputText>
							</p:column>
                    
                    </p:dataTable>
             <h:graphicImage   value="#{grafikData.duranVarliklarGrafikD.grafik}" />
             </h:panelGrid>
           </p:tab>
             <p:tab title="Pasif Dağılım">
              
             <h:panelGrid columns="2">
                    <p:dataTable  value="#{grafikData.kisaVadeliYabanciGrafikD.grafikModels}" var="varlik" style="width:550px">
                           	<p:column style="width:390px">
                           		<f:facet name="header">
									<h:outputText styleClass="outputL" value="Kısa Vadeli Yabancı Kaynaklar"></h:outputText>
								</f:facet>
								<h:outputText styleClass="outputL" value="#{varlik.name}" />
							</p:column>

							<p:column>
								<f:facet name="header">
									<h:outputText styleClass="outputL" value="Cari Dönem"></h:outputText>
								</f:facet>
								<h:inputText value="#{varlik.cari}" disabled="true"	style="text-align:right" size="15">

									<f:convertNumber pattern="#,##0.00"
										locale="#{sesionBean.trlocal}" />
									
								</h:inputText>
							</p:column>
								<p:column>
								<f:facet name="header">
									<h:outputText styleClass="outputL" value="Yüzde(%)"></h:outputText>
								</f:facet>
								<h:inputText value="#{varlik.percent}" disabled="true"	style="text-align:right" size="5">

																		
								</h:inputText>
							</p:column>
                    
                    </p:dataTable>
             <h:graphicImage   value="#{grafikData.kisaVadeliYabanciGrafikD.grafik}" />
         
                    <p:dataTable  value="#{grafikData.uzunVadeliYabanciGrafikD.grafikModels}" var="varlik" style="width:550px">
                           	<p:column style="width:390px">
                           		<f:facet name="header">
									<h:outputText styleClass="outputL" value="Uzun Vadeli Yabancı Kaynaklar"></h:outputText>
								</f:facet>
								<h:outputText styleClass="outputL" value="#{varlik.name}"  />
							</p:column>

							<p:column>
								<f:facet name="header">
									<h:outputText styleClass="outputL" value="Cari Dönem"></h:outputText>
								</f:facet>
								<h:inputText value="#{varlik.cari}" disabled="true"	style="text-align:right" size="15">

									<f:convertNumber pattern="#,##0.00"
										locale="#{sesionBean.trlocal}" />
									
								</h:inputText>
							</p:column>
								<p:column>
								<f:facet name="header">
									<h:outputText styleClass="outputL" value="Yüzde(%)"></h:outputText>
								</f:facet>
								<h:inputText value="#{varlik.percent}" disabled="true"	style="text-align:right" size="5">

																		
								</h:inputText>
							</p:column>
                    
                    </p:dataTable>
             <h:graphicImage   value="#{grafikData.uzunVadeliYabanciGrafikD.grafik}" />
         
                    <p:dataTable  value="#{grafikData.ozkanyaklarGrafikD.grafikModels}" var="varlik" style="width:550px">
                           	<p:column style="width:390px">
                           		<f:facet name="header">
									<h:outputText styleClass="outputL" value="Özkaynaklar"></h:outputText>
								</f:facet>
								<h:outputText styleClass="outputL" value="#{varlik.name}" />
							</p:column>

							<p:column>
								<f:facet name="header">
									<h:outputText styleClass="outputL" value="Cari Dönem"></h:outputText>
								</f:facet>
								<h:inputText value="#{varlik.cari}" disabled="true"	style="text-align:right" size="15">

									<f:convertNumber pattern="#,##0.00"
										locale="#{sesionBean.trlocal}" />
									
								</h:inputText>
							</p:column>
								<p:column>
								<f:facet name="header">
									<h:outputText styleClass="outputL" value="Yüzde(%)"></h:outputText>
								</f:facet>
								<h:inputText value="#{varlik.percent}" disabled="true"	style="text-align:right" size="5">

																		
								</h:inputText>
							</p:column>
                    
                    </p:dataTable>
             <h:graphicImage   value="#{grafikData.ozkanyaklarGrafikD.grafik}" />
             </h:panelGrid>
           </p:tab>
        </p:tabView>
	</h:form>
	</ui:define>

</ui:composition>