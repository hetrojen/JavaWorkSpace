package com.company.rating.form;

import java.math.BigDecimal;


public class RatingViewData {
   private boolean kantitatifHesButDis;
   private BigDecimal kantitatifToplam=new BigDecimal(0);
   private BigDecimal  kalitatifToplam=new BigDecimal(0);;
   private  int coloumnNum;

public boolean isKantitatifHesButDis() {
	return kantitatifHesButDis;
}

public void setKantitatifHesButDis(boolean kantitatifHesButDis) {
	this.kantitatifHesButDis = kantitatifHesButDis;
}




public int getColoumnNum() {
	if(kantitatifHesButDis){
		coloumnNum--;
	}
	return coloumnNum;
}

public void setColoumnNum(int coloumnNum) {
	this.coloumnNum = coloumnNum;
}

public BigDecimal getKantitatifToplam() {
	return kantitatifToplam;
}

public void setKantitatifToplam(BigDecimal kantitatifToplam) {
	this.kantitatifToplam = kantitatifToplam;
}

public BigDecimal getKalitatifToplam() {
	return kalitatifToplam;
}

public void setKalitatifToplam(BigDecimal kalitatifToplam) {
	this.kalitatifToplam = kalitatifToplam;
}


   
}
